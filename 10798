#include <iostream>

using namespace std;

int main(void){

    char mat[5][15] = {NULL,};
    for(int i=0;i<5;i++)
    {
        cin>>mat[i];
    }
    for(int i=0;i<15;i++)
    {
        for(int j=0;j<5;j++)
        {
            if(mat[j][i]==NULL) continue;
            else cout<<mat[j][i];
        }
    }  
}
