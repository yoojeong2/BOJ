#include <string>
#include <iostream>

using namespace std;

int main(void){

int ball[101] = {0};
int N,M;
cin>>N>>M;

for(int i=0;i<M;i++){
    int a,b,c;
    cin>>a>>b>>c;
    for(int j=a;j<=b;j++) ball[j] = c;
}

for (int i=1;i<=N;i++){
    if(i==N)cout<<ball[i]<<endl;
    else cout<<ball[i]<<" ";
} //init

}
