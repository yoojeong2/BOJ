#include <stdio.h>
#include <string>

int main(void){
    int T,n;
    char s[21];
    scanf("%d",&T);
    for(int i=0;i<T;i++){
        scanf("%d %s",&n,s);
        for(int j=0;s[j]!='\0';j++){
            for(int k=0;k<n;k++){
                printf("%c",s[j]);    
            }
        }
        printf("\n");
    }
}
